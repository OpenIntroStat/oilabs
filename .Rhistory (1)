# Chunk 1: setup
library(dplyr)
library(ggplot2)
T <- read.table("~/thesis/r stuff/T_master.txt", header = TRUE, sep = "\t")
# Chunk 2
qplot(data = T, x = days, y = testosterone, col = fish, geom = "line", ylab = "testosterone pg/g/hr", xlab = "days") + theme_bw()
# Chunk 3
filtered_T <- filter(.data = T, fish == "S1LG1" | fish == "B1L1" | fish == "B1R1")
qplot(data = filtered_T, x = days, y = testosterone, col = fish, geom = "line", ylab = "testosterone pg/g/hr", xlab = "days") + theme_bw()
# Chunk 4
filtered_T2 <- T %>% filter(fish == "S1LG1" | fish == "B1L1" | fish == "B1R1") %>%
mutate(stage = factor(stage, levels = c("gravid", "mouthbrooding eggs", "mouthbrooding fry", "post release care", "post release no care")))
qplot(data = filtered_T2, x = fish, y = testosterone,
col = fish, geom = "point", ylab = "testosterone pg/g/hr") + facet_grid(. ~ stage) +
theme_bw()
